<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <title><%= data.page.title || "The Middleman" %></title>    
    <%= stylesheet_link_tag "normalize", "all" %>    
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
    
    
    
    
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <%= javascript_include_tag  "jquery.tmpl.min", "underscore-1.1.7", "backbone", "backbone.queryparam", "all" %>
    
    <script type="text/template" id='document_template'>
      <li class="menu-item">
            <h4>${ attributes.firstname + ' ' + attributes.lastname }</h4>
            <p><img src="${ attributes.imageURL }" alt=""></p>
      </li>
    </script>


  </body>
</html>